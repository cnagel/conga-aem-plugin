<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/content/dam/wcmio-aem-archetype">
        <!-- Exclude folder thumbnails - they will be generated automatically -->
        <exclude pattern="^.*/jcr:content/folderThumbnail(/.*)?$"/>
    </filter>
    <filter root="/content/wcmio-aem-archetype"/>
    <filter root="/content/experience-fragments/wcmio-aem-archetype"/>
    <filter root="/conf/wcmio-aem-archetype">
        <include pattern="/conf/wcmio-aem-archetype/.*"/>
        <exclude pattern="/conf/wcmio-aem-archetype/rep:policy(/.*)?"/>
        <exclude pattern="/conf/wcmio-aem-archetype/settings/wcm(/.*)?"/>
    </filter>
</workspaceFilter>
